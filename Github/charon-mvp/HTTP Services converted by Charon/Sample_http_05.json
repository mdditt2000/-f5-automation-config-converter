{
  "class": "AS3",
  "action": "deploy",
  "persist": true,
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.0.0",
    "id": "zyxwu8675309",
    "label": "Sample 5",
    "remark": "Two applications sharing a pool",
    "Sample_http_05": {
      "class": "Tenant",
      "Shared": {
        "class": "Application",
        "template": "shared",
        "dual_pool": {
          "members": [
            {
              "addressDiscovery": "static",
              "servicePort": 80,
              "serverAddresses": [
                "192.0.10.10",
                "192.0.10.11"
              ],
              "shareNodes": true
            }
          ],
          "minimumMembersActive": 1,
          "class": "Pool",
          "minimumMonitors": 1,
          "monitors": [
            {
              "bigip": "/Common/http"
            }
          ]
        }
      },
      "A1": {
        "class": "Application",
        "template": "generic",
        "service": {
          "remark": "A1",
          "layer4": "tcp",
          "persistenceMethods": [
            "cookie"
          ],
          "pool": "/Sample_http_05/Shared/dual_pool",
          "translateServerAddress": true,
          "translateServerPort": true,
          "class": "Service_HTTP",
          "profileTCP": {
            "bigip": "/Common/f5-tcp-progressive"
          },
          "profileHTTP": {
            "bigip": "/Common/http"
          },
          "virtualAddresses": [
            "10.0.10.10"
          ],
          "virtualPort": 80,
          "snat": "auto"
        }
      },
      "A2": {
        "class": "Application",
        "template": "generic",
        "service": {
          "remark": "A2",
          "layer4": "tcp",
          "persistenceMethods": [
            "cookie"
          ],
          "pool": "/Sample_http_05/Shared/dual_pool",
          "translateServerAddress": true,
          "translateServerPort": true,
          "class": "Service_HTTPS",
          "profileTCP": {
            "bigip": "/Common/f5-tcp-progressive"
          },
          "profileHTTP": {
            "bigip": "/Common/http"
          },
          "serverTLS": "webtls",
          "virtualAddresses": [
            "10.0.10.20"
          ],
          "virtualPort": 443,
          "snat": "auto"
        },
        "webtls": {
          "allowExpiredCRL": false,
          "authenticationFrequency": "one-time",
          "c3dOCSPUnknownStatusAction": "drop",
          "certificates": [
            {
              "certificate": "/Sample_http_05/A2/webcert"
            }
          ],
          "cacheCertificateEnabled": false,
          "ciphers": "DEFAULT",
          "remark": "none",
          "staplerOCSPEnabled": false,
          "authenticationMode": "ignore",
          "renegotiationEnabled": true,
          "requireSNI": false,
          "c3dEnabled": false,
          "forwardProxyEnabled": false,
          "forwardProxyBypassEnabled": false,
          "class": "TLS_Server",
          "tls1_0Enabled": true,
          "tls1_1Enabled": true,
          "tls1_2Enabled": true,
          "tls1_3Enabled": false,
          "singleUseDhEnabled": false,
          "insertEmptyFragmentsEnabled": false
